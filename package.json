{
	"name": "kotoba",
	"version": "0.1.0",
	"description": "Helper tool for Japanese study",
	"main": "build/main/index.js",
	"private": true,
	"scripts": {
		"start": "run-p start:*",
		"main": "ts-node ./src/main.ts",
		"build": "run-p build:*",
		"build:ts": "tsc",
		"build:web": "webpack --mode development",
		"test": "jest",
		"start:web": "webpack serve --env devServer --mode development --hot",
		"start:server": "nodemon",
		"check": "run-s check:* lint",
		"check:types": "tsc --noemit",
		"check:tests": "jest --coverage --coverageReporters text html",
		"lint": "eslint -f codeframe . --ext .ts,.tsx ",
		"open-coverage": "run-s open-coverage:*",
		"open-coverage:windows": "open-cli build\\coverage\\index.html",
		"open-coverage:default": "open-cli build/coverage/index.html",
		"import": "ts-node ./importer/import.ts",
		"kana-test": "ts-node ./src/kana-test.ts"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:ax-lab/kotoba.git"
	},
	"keywords": [
		"japanese"
	],
	"author": "axemac",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.13.14",
		"@babel/preset-env": "^7.13.12",
		"@babel/preset-typescript": "^7.13.0",
		"@types/better-sqlite3": "^5.4.1",
		"@types/express": "^4.17.11",
		"@types/jest": "^26.0.22",
		"@types/node": "^14.14.37",
		"@types/react": "^17.0.3",
		"@types/react-dom": "^17.0.3",
		"@types/react-router-dom": "^5.1.7",
		"@types/sax": "^1.2.1",
		"@types/seedrandom": "^3.0.1",
		"@types/sqlite3": "^3.1.7",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.20.0",
		"@typescript-eslint/parser": "^4.20.0",
		"copy-webpack-plugin": "^8.1.0",
		"css-loader": "^5.2.0",
		"eslint": "^7.23.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.3.2",
		"eslint-plugin-only-warn": "^1.0.2",
		"jest": "^26.6.3",
		"nodemon": "^2.0.7",
		"npm-run-all": "^4.1.5",
		"open-cli": "^6.0.1",
		"prettier": "^2.2.1",
		"sass": "^1.32.8",
		"sass-loader": "^11.0.1",
		"style-loader": "^2.0.0",
		"ts-loader": "^8.1.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.3",
		"webpack": "^5.28.0",
		"webpack-cli": "^4.7.0",
		"webpack-dev-server": "^4.3.0"
	},
	"dependencies": {
		"better-sqlite3": "^7.4.1",
		"body-parser": "^1.19.0",
		"classnames": "^2.3.1",
		"express": "^4.17.1",
		"express-graphql": "^0.12.0",
		"graphql": "^15.5.0",
		"immutable": "^4.0.0-rc.12",
		"jszip": "^3.6.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.2.0",
		"sax": "^1.2.4",
		"seedrandom": "^3.0.5",
		"sqlite3": "^4.2.0",
		"uuid": "^8.3.2"
	}
}
