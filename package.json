{
	"name": "kotoba",
	"version": "0.1.0",
	"description": "Helper tool for Japanese study",
	"main": "build/main/index.js",
	"private": true,
	"scripts": {
		"start": "run-p start:*",
		"main": "ts-node ./server-node/main.ts",
		"build": "run-p build:*",
		"build:ts": "tsc",
		"build:web": "webpack --mode development",
		"test": "jest",
		"start:web": "webpack serve --env devServer --mode development --hot",
		"start:server": "nodemon",
		"check": "run-s check:* lint",
		"check:types": "tsc --noemit",
		"check:tests": "jest --coverage --coverageReporters text html",
		"lint": "eslint -f codeframe . --ext .ts,.tsx ",
		"open-coverage": "run-s open-coverage:*",
		"open-coverage:windows": "open-cli build\\coverage\\index.html",
		"open-coverage:default": "open-cli build/coverage/index.html",
		"import": "ts-node ./importer/import.ts",
		"kana-test": "ts-node ./server-node/kana-test.ts",
		"rust": "run-p rust:*",
		"rust:watch-check": "cargo watch --workdir kotoba-server -x check -s \"touch .trigger\"",
		"rust:watch-run": "cargo watch --no-gitignore --workdir kotoba-server -w .trigger -x run"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:ax-lab/kotoba.git"
	},
	"keywords": [
		"japanese"
	],
	"author": "ax-lab",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.19.1",
		"@babel/preset-env": "^7.19.1",
		"@babel/preset-typescript": "^7.18.6",
		"@types/adm-zip": "^0.5.0",
		"@types/better-sqlite3": "^5.4.3",
		"@types/express": "^4.17.14",
		"@types/jest": "^26.0.24",
		"@types/node": "^14.18.29",
		"@types/react": "^17.0.50",
		"@types/react-dom": "^17.0.17",
		"@types/react-router-dom": "^5.3.3",
		"@types/sax": "^1.2.4",
		"@types/seedrandom": "^3.0.2",
		"@types/sqlite3": "^3.1.8",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"copy-webpack-plugin": "^8.1.1",
		"css-loader": "^5.2.7",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^24.7.0",
		"eslint-plugin-only-warn": "^1.0.3",
		"jest": "^26.6.3",
		"nodemon": "^2.0.20",
		"npm-run-all": "^4.1.5",
		"open-cli": "^6.0.1",
		"prettier": "^2.7.1",
		"sass": "^1.54.9",
		"sass-loader": "^11.1.1",
		"style-loader": "^2.0.0",
		"ts-loader": "^8.4.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.3",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.11.1"
	},
	"dependencies": {
		"adm-zip": "^0.5.9",
		"better-sqlite3": "^7.6.2",
		"body-parser": "^1.19.0",
		"classnames": "^2.3.2",
		"express": "^4.18.1",
		"express-graphql": "^0.12.0",
		"graphql": "^15.8.0",
		"immutable": "^4.1.0",
		"jszip": "^3.10.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.3.3",
		"sax": "^1.2.4",
		"seedrandom": "^3.0.5",
		"sqlite3": "^4.2.0",
		"uuid": "^8.3.2"
	}
}
